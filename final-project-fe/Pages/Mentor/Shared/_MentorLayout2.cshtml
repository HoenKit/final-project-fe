        <!-- ========== header end ========== -->
        <!-- ========== section start ========== -->
        <section class="section">
            <div class="container-fluid">
                @RenderBody()
                @Html.Partial("_Notification")
            </div>
            <!-- end container -->
        </section>
        <!-- ========== section end ========== -->
